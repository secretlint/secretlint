{
  "name": "@secretlint/secretlint-rule-preset-canary",
  "version": "6.0.2",
  "description": "Canary rule preset of secretlint.",
  "keywords": [
    "secretlint",
    "rule",
    "rule-preset"
  ],
  "homepage": "https://github.com/secretlint/secretlint/tree/master/packages/@secretlint/secretlint-rule-preset-canary/",
  "bugs": {
    "url": "https://github.com/secretlint/secretlint/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/secretlint/secretlint.git"
  },
  "license": "MIT",
  "author": "azu",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "scripts": {
    "build": "rollup --config",
    "build:tsc": "tsc -p .",
    "clean": "rimraf lib/",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "updateSnapshot": "UPDATE_SNAPSHOT=1 npm test",
    "watch": "tsc -p . --watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "dependencies": {
    "@secretlint/secretlint-rule-aws": "^6.0.2",
    "@secretlint/secretlint-rule-basicauth": "^6.0.2",
    "@secretlint/secretlint-rule-filter-comments": "^6.0.2",
    "@secretlint/secretlint-rule-gcp": "^6.0.2",
    "@secretlint/secretlint-rule-github": "^6.0.2",
    "@secretlint/secretlint-rule-npm": "^6.0.2",
    "@secretlint/secretlint-rule-privatekey": "^6.0.2",
    "@secretlint/secretlint-rule-sendgrid": "^6.0.2",
    "@secretlint/secretlint-rule-shopify": "^6.0.2",
    "@secretlint/secretlint-rule-slack": "^6.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@secretlint/tester": "^6.0.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "mocha": "^10.2.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.10.0",
    "ts-node": "^10.9.1",
    "ts-node-test-register": "^10.0.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
