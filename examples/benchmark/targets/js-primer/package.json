{
  "name": "js-primer",
  "private": true,
  "license": "MIT",
  "version": "1.0.0",
  "description": "JavaScriptの入門本",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublish": "npm run install:gitbook",
    "install:gitbook": "gitbook install",
    "start": "gitbook serve",
    "build": "npm-run-all build:web",
    "build:all": "npm-run-all build:web build:pdf build:epub build:mobi",
    "build:web": "gitbook build",
    "build:pdf": "gitbook pdf ./ _book/javascript-primer.pdf",
    "build:epub": "gitbook epub ./ _book/javascript-primer.epub",
    "build:mobi": "gitbook mobi ./ _book/javascript-primer.mobi",
    "postbuild": "npm-run-all build:add-resource workbox",
    "workbox": "workbox injectManifest workbox-config.js",
    "build:add-resource": "node tools/gitbook/copy-favicon.js",
    "eslint": "eslint source/ --cache",
    "eslint:fix": "eslint --fix source/ --cache",
    "prettier:html": "prettier \"source/use-case/**/*.html\" --write",
    "textlint": "summary-to-path ./source/README.md | xargs textlint -f pretty-error --cache",
    "textlint:no-cached": "summary-to-path ./source/README.md | xargs textlint -f pretty-error",
    "textlint:fix": "summary-to-path ./source/README.md | xargs textlint --fix",
    "textlint-link": "summary-to-path ./source/README.md | LINK_CHECK=true xargs textlint",
    "textlint-link:fix": "summary-to-path ./source/README.md | LINK_CHECK=true xargs textlint --fix",
    "lint": "npm-run-all --parallel textlint eslint",
    "lint:fix": "npm-run-all --parallel textlint:fix eslint:fix",
    "test:example": "mocha test/",
    "test:basic": "mocha \"./source/basic/**/*-test.js\"",
    "test:ajaxapp": "mocha \"./source/use-case/ajaxapp/!(node_modules)**/*-test.js\"",
    "test:todoapp": "cd ./source/use-case/todoapp && npm install && npm test",
    "e2e:todoapp": "cd ./source/use-case/todoapp && npm install && npm run e2e",
    "test": "npm-run-all -l --parallel test:* lint",
    "e2e": "npm-run-all -l --parallel e2e:*",
    "travis": "npm-run-all -l --parallel test build",
    "build:applescript": "tsc -p tools/applescript"
  },
  "keywords": [
    "gitbook",
    "textlint"
  ],
  "devDependencies": {
    "@alrra/travis-scripts": "^3.0.1",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "@jxa/global-type": "^1.0.1",
    "@jxa/run": "^1.0.3",
    "@power-doctest/javascript": "^5.0.1",
    "@power-doctest/markdown": "^5.0.1",
    "@power-doctest/tester": "^5.0.1",
    "@textlint-ja/textlint-rule-no-synonyms": "^1.0.1",
    "@textlint-rule/textlint-rule-no-invalid-control-character": "^1.0.1",
    "@textlint-rule/textlint-rule-no-unmatched-pair": "^1.0.7",
    "@textlint-rule/textlint-rule-require-header-id": "^1.0.1",
    "@textlint/regexp-string-matcher": "^1.0.2",
    "@types/meow": "^4.0.1",
    "@types/node": "^9.6.52",
    "add-text-to-markdown": "^2.0.0",
    "babel-preset-jsdoc-to-assert": "^5.0.0",
    "babel-preset-power-assert": "^3.0.0",
    "consolemock": "^1.1.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "esprima": "^4.0.0",
    "esquery": "^1.0.0",
    "front-matter": "^2.1.0",
    "gitbook-cli": "^2.1.2",
    "gitbook-plugin-anchors": "^0.7.1",
    "gitbook-plugin-canonical-link": "^2.0.2",
    "gitbook-plugin-custom-favicon": "0.0.4",
    "gitbook-plugin-ga": "^1.0.0",
    "gitbook-plugin-github-issue-feedback": "^1.1.1",
    "gitbook-plugin-include-codeblock": "^3.0.0",
    "gitbook-plugin-js-console": "^2.0.4",
    "gitbook-plugin-page-toc-button": "^0.1.1",
    "gitbook-summary-to-path": "^1.1.0",
    "globby": "^6.0.0",
    "js-levenshtein": "^1.1.3",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "lodash": "^4.17.15",
    "meow": "^5.0.0",
    "micromatch": "^3.1.10",
    "mocha": "^4.0.0",
    "npm-run-all": "^4.0.0",
    "power-assert": "^1.6.1",
    "prettier": "^1.18.0",
    "remark": "^8.0.0",
    "rewire": "^2.5.2",
    "strict-eval": "^1.0.2",
    "sudachi-synonyms-dictionary": "^4.1.0",
    "text-testing-mocha": "^0.4.0",
    "textlint": "^11.4.0",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-filter-rule-whitelist": "^2.0.0",
    "textlint-rule-eslint": "^3.2.0",
    "textlint-rule-footnote-order": "^1.0.2",
    "textlint-rule-inline-code-denylist": "file:./textlint/textlint-rule-inline-code-denylist",
    "textlint-rule-max-comma": "^1.0.4",
    "textlint-rule-no-dead-link": "^4.6.1",
    "textlint-rule-no-doubled-joshi": "^3.7.2",
    "textlint-rule-no-js-function-paren": "^1.0.1",
    "textlint-rule-period-in-list-item": "^0.3.2",
    "textlint-rule-preset-ja-technical-writing": "^3.1.2",
    "textlint-rule-prh": "^5.3.0",
    "textlint-rule-sentence-length": "^2.2.0",
    "textstat": "^0.8.0",
    "typescript": "^2.8.3",
    "unist-util-find-all-between": "^1.0.6",
    "unist-util-find-before": "^2.0.4",
    "unist-util-parents": "^1.0.2",
    "unist-util-select": "^1.5.0",
    "vm2": "^3.8.2",
    "wait-on": "^3.3.0",
    "workbox-cli": "^3.2.0"
  },
  "dependencies": {}
}
